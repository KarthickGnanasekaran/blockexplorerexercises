pragma solidity >=0.7.0 <0.9.0;

contract Leapyear {
    uint year;

    function getleapyear(uint _year) public view returns (string memory){
        if (_year % 4 == 0) {
            return("Year is leap year");}
        else {
            return("Year is not leap year");
        }
    }
}
